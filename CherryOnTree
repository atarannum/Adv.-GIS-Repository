import random
SpinOutcome = [-1, -2, -3, -4, 2, 2, 10]
random.randrange(0, 7)
SpinOutcome[random.randrange(0, 7)] 
CherriesOnTree = 10
attempts = 0

while CherriesOnTree > 0:
    Spin = SpinOutcome[random.randrange(0, 7)]
    CherriesOnTree = CherriesOnTree + Spin
    print(f"You spun {Spin}")
    print(f"{CherriesOnTree} Cheeries on tree left")
    if CherriesOnTree > 10:
        CherriesOnTree = 10
        print(f"You have now {CherriesOnTree} cherries on your tree")
    elif CherriesOnTree < 0:
        CherriesOnTree = 0
        print(f"You have {CherriesOnTree} cherries on your tree, Hi Ho Cherrie-O!")
    attempts += 1
    print(f"You spun {attempts} times")
